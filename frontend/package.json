{
  "private": true,
  "name": "dailies",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "node-sass": "^4.12.0",
    "preact-cli": "^3.0.0-rc.4",
    "sass-loader": "^7.1.0"
  },
  "dependencies": {
    "dexie": "^2.0.4",
    "fast-async": "^6.3.8",
    "preact": "^10.0.0-rc.1",
    "preact-render-to-string": "^5.0.6",
    "preact-router": "^3.0.1",
    "react-feather": "^1.1.6",
    "unistore": "^3.4.1"
  }
}
